@model MvcCv.Models.Entity.TblEgitimlerim
@{
    ViewBag.Title = "EgitimEkle";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h4>Yeni Eğitim Ekleme Sayfası</h4>
<br />
<form method="post">
    @Html.Label("Üniversite: ")
    @Html.TextBoxFor(x => x.Baslik, new { @class = "form-control" })
    @Html.ValidationMessageFor(x => x.Baslik, "", new { @style = "color:red" })
    <br />
    @Html.Label("Fakülte: ")
    @Html.TextBoxFor(x => x.AltBaslik1, new { @class = "form-control" })
    @Html.ValidationMessageFor(x => x.AltBaslik1, "", new { @style = "color:red" })
    <br />
    @Html.Label("Bölüm: ")
    @Html.TextBoxFor(x => x.AltBaslik2, new { @class = "form-control" })
    @Html.ValidationMessageFor(x => x.AltBaslik2, "", new { @style = "color:red" })
    <br />
    @Html.Label("Genel Not Ortalaması(GNO): ")
    @Html.TextBoxFor(x => x.GNO, new { @class = "form-control" })
    @Html.ValidationMessageFor(x => x.GNO, "", new { @style = "color:red" })
    <br />
    @Html.Label("Tarih: ")
    @Html.TextBoxFor(x => x.Tarih, new { @class = "form-control" })
    <br />
    <button class="btn btn-info">Kaydet</button>
</form>


